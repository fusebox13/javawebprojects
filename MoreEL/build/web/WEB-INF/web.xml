<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    
    <context-param>
      <param-name>mainEmail</param-name>
      <param-value>likewecare@wickedlysmart.com</param-value>
    </context-param>
    
    <servlet>
        <servlet-name>CallingServlet</servlet-name>
        <servlet-class>myservlets.CallingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CallingServlet2</servlet-name>
        <servlet-class>myservlets.CallingServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CallingServlet3</servlet-name>
        <servlet-class>myservlets.CallingServlet3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CallingServlet</servlet-name>
        <url-pattern>/CallingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CallingServlet2</servlet-name>
        <url-pattern>/CallingServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CallingServlet3</servlet-name>
        <url-pattern>/CallingServlet3</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
